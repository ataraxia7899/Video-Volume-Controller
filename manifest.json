{
	"manifest_version": 3,
	"name": "__MSG_extName__",
	"version": "1.0.6",
	"description": "__MSG_extDescription__",
	"default_locale": "en",
	"permissions": ["storage", "activeTab", "tabCapture", "offscreen"],
	"host_permissions": ["*://*/*"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html",
